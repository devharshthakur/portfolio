{
	"plugins": ["prettier-plugin-tailwindcss"],
	"semi": true,
	"singleQuote": false,
	"tabWidth": 2,
	"useTabs": true,
	"printWidth": 100,
	"trailingComma": "es5",
	"bracketSpacing": true,
	"bracketSameLine": false,
	"arrowParens": "always",
	"endOfLine": "lf",
	"htmlWhitespaceSensitivity": "css",
	"embeddedLanguageFormatting": "auto"
}
